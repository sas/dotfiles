# fsfe
Host gcc60
  Port 9200
Host gcc61
  Port 9201
Host gcc62
  Port 9202
Host gcc63
  Port 9203
Host gcc64
  Port 9204
Host gcc66
  Port 9206
Host gcc*
  User thrashboul
  HostName %h.fsffrance.org
  IdentityFile ~/Documents/keys/ssh/fsfe.priv

Host *
  CheckHostIP no
  HashKnownHosts no
  HostKeyAlgorithms ssh-rsa,ssh-dss

  Compression yes
  ServerAliveInterval 30

  ControlPath ~/.ssh/control/%r@%h:%p
  ControlMaster auto
  ControlPersist 10m
